import com.kaydend.testmod.help.RegisterHelper;

import net.minecraft.item.Item;

public class ModItems 
{
	public static Item copperIngot = new testModItem().setUnlocalizedName("copperIngot").setTextureName(Reference.modid + ":" + copper_ingot.png);
	
	public static void init()
	{
	
    	RegisterHelper.registerItem(copperIngot);
	}
}
