package com.kaydend.testmod.blocks;

import com.kaydend.testmod.help.Reference;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;
import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class copper_Block extends Block 
{
	
		public copper_Block()
		{
			super(Material.rock);
			setBlockName(Reference.modid + ":" + "copper_block");
			setCreativeTab(CreativeTabs.tabBlock);
			setBlockTextureName("kaydend_testmod" + ";" + "copper_Block");
		}



}
